{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "firstname": {
      "type": "string"
    },
    "lastname": {
      "type": "string"
    },
    "gender": {
      "type": "string",
      "pattern": "(male|female)"
    },
    "dob": {
      "type": "string",
      "pattern": "(0[1-9]|1[0-2])-([0-2][0-9]|3[0-1])-[0-9]{4}"
    },
    "id": {
      "type": "string",
      "pattern": "CST[0-9]{7}"
    }
  },
  "required": [
    "firstname",
    "lastname",
    "id"
  ]
}